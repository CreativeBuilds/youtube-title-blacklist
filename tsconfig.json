{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "declaration": false,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {},
    "typeRoots": ["node_modules/@types"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src"],
  "files": [
    "./vue-shims.d.ts",
    "./src/content/USER_SETTINGS.ts",
    "./src/content/check_for_blacklisted_videos.ts",
    "./src/content/fill_in_missing_video.ts",
    "./src/content/getParentFromTypeOrNull.ts",
    "./src/content/get_user_blacklist.ts",
    "./src/content/addListeners.ts"
  ]
}
